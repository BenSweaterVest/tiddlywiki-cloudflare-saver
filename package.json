{
  "name": "@bensweetervest/tiddlywiki-cloudflare-saver",
  "version": "1.0.0",
  "description": "TiddlyWiki plugin for saving to GitHub via Cloudflare Functions",
  "main": "dist/cloudflare-saver-plugin.json",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "validate": "node scripts/validate.js",
    "test": "jest --passWithNoTests",
    "dev": "npm run build && npm run validate",
    "prepare": "npm run build",
    "preversion": "npm test && npm run build && npm run validate",
    "version": "npm run build && git add dist/",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "tiddlywiki",
    "plugin", 
    "cloudflare",
    "github",
    "saver"
  ],
  "author": "BenSweaterVest",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BenSweaterVest/tiddlywiki-cloudflare-saver.git"
  },
  "bugs": {
    "url": "https://github.com/BenSweaterVest/tiddlywiki-cloudflare-saver/issues"
  },
  "homepage": "https://github.com/BenSweaterVest/tiddlywiki-cloudflare-saver#readme",
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "jest": "^29.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
