{
  "name": "@bensweetervest/tiddlywiki-cloudflare-saver",
  "version": "1.1.1",
  "description": "TiddlyWiki plugin for saving to GitHub via Cloudflare Functions",
  "main": "dist/cloudflare-saver-plugin.json",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "validate": "node scripts/validate.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ demo/ scripts/ __tests__/",
    "lint:fix": "eslint src/ demo/ scripts/ __tests__/ --fix",
    "dev": "npm run build && npm run validate",
    "prepare": "npm run build",
    "preversion": "npm run lint && npm test && npm run build && npm run validate",
    "version": "npm run build && git add dist/",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "tiddlywiki",
    "tiddlywiki-plugin",
    "plugin",
    "cloudflare",
    "cloudflare-functions",
    "cloudflare-pages",
    "github",
    "saver",
    "wiki",
    "notes",
    "serverless",
    "auto-save",
    "backup"
  ],
  "author": "BenSweaterVest",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BenSweaterVest/tiddlywiki-cloudflare-saver.git"
  },
  "bugs": {
    "url": "https://github.com/BenSweaterVest/tiddlywiki-cloudflare-saver/issues"
  },
  "homepage": "https://github.com/BenSweaterVest/tiddlywiki-cloudflare-saver#readme",
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9.39.2",
    "jest": "^29.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
